{
  "name": "fasttrack.ourstuff.space",
  "version": "1.0.0",
  "type": "module",
  "description": "## Firebase configuration",
  "main": "firebase-config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "biome format --write .",
    "lint": "npm run lint:biome && npm run lint:html && npm run lint:css && npm run lint:js",
    "lint:biome": "biome check --write .",
    "lint:html": "html-validate index.html",
    "lint:html:vnu": "java -jar node_modules/vnu-jar/build/dist/vnu.jar --skip-non-html index.html",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:js": "eslint \"**/*.js\"",
    "lint:fix": "npm run lint:biome && stylelint --fix \"**/*.css\" && eslint --fix \"**/*.js\"",
    "validate": "npm run format && npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "eslint": "^9.39.2",
    "html-validate": "^10.6.0",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "vnu-jar": "^26.1.11"
  }
}
