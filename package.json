{
  "name": "fasttrack.ourstuff.space",
  "version": "1.0.0",
  "description": "## Firebase configuration",
  "main": "firebase-config.js",
  "type": "module",
  "scripts": {
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint .",
    "biome:check": "biome check --write .",
    "biome": "npm run biome:check",
    "html-validate": "html-validate '*.html'",
    "vnu": "java -jar node_modules/vnu-jar/build/dist/vnu.jar --also-check-css --also-check-svg --skip-non-html index.html",
    "stylelint": "stylelint '**/*.css'",
    "stylelint:fix": "stylelint '**/*.css' --fix",
    "eslint": "eslint '*.js' || true",
    "eslint:fix": "eslint '*.js' --fix || true",
    "lint": "npm run biome:lint && npm run html-validate && npm run stylelint && npm run eslint",
    "format": "npm run biome:format && npm run stylelint:fix && npm run eslint:fix",
    "validate": "npm run vnu",
    "check": "npm run format && npm run lint && npm run validate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "html-validate": "^10.6.0",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "vnu-jar": "^26.1.11"
  }
}
